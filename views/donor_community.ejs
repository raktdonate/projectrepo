<%- include('./includes/head.ejs')%>
<link rel="stylesheet" href="css/donorcommunity.css">
</head>
<body>
    <%- include('./includes/navbar.ejs')%> 
    <% if(userData.length>=0){%>
    <ul>
        <% for(let user of userData){%>
        <li class="listdc">
            <div class="listdcdiv">
                <p class="info">Name</p>
                <p><%=user.username%></p>
            </div>
            <div class="listdcdiv">
                <p class="info">City</p>
                <p><%=user.city%></p>
            </div>
            <div class="listdcdiv">
                <p class="info">Chat</p>
                <p>Click here to chat</p>
            </div>
            <div class="listdcdiv">
                <p class="info">Contact Number</p>
                <p><%=user.contact%></p>
            </div>
            <div class="listdcdiv">
                <p class="info">Email</p>
                <p><%=user.email%></p>
            </div>
        </li>
        <%}%>
    </ul>
    <%}else{%>
        <p>empty community</p>
    <%}%>
    
<%- include('./includes/tail.ejs')%>